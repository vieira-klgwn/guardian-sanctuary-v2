{
    "builds": [
      {
        "src": "api/scanner.py",
        "use": "@vercel/python"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/scan",
        "dest": "api/scanner.py"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1"
      }
    ]
  }